{
  "name": "outlet-shop",
  "description": "Full-featured shop app built with Outlet Client.",
  "version": "1.0.0",
  "license": "MIT",
  "main": "./dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/combine/saga.git"
  },
  "engines": {
    "node": "8.x.x",
    "npm": "5.2.x"
  },
  "_cssModulesId": "[name]__[local]__[hash:base64:5]",
  "_moduleAliases": {
    "@assets": "app/assets",
    "@common": "app/components/common",
    "@components": "app/components",
    "@config": "config",
    "@containers": "app/containers",
    "@hocs": "app/hocs",
    "@lib": "app/lib",
    "@pages": "app/pages",
    "@routes": "app/routes",
    "@themes": "app/themes"
  },
  "scripts": {
    "start": "bnr start",
    "serve": "bnr serve",
    "build": "bnr build",
    "pretest": "bnr pretest --path=null",
    "test": "bnr --path=null test",
    "test:all": "bnr --path=null test:all",
    "test:watch": "bnr --path=null test:watch",
    "test:all:watch": "bnr --path=null test:all:watch",
    "lint": "bnr lint",
    "heroku-postbuild": "bnr prod:build"
  },
  "betterScripts": {
    "serve": {
      "command": "(cd ./dist && node ./index.js)",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "start": {
      "command": "rm -rf ./dist && (cd ./src && node ./index)",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": true
      }
    },
    "build": {
      "command": "npx webpack --config webpack/client.production.babel.js --progress && babel ./src -d ./dist",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "pretest": {
      "command": "node ./test/support/init.js",
      "env": {
        "NODE_ENV": "test"
      }
    },
    "test": {
      "command": "npx jest -c ./test/support/jest.config.js --verbose --detectOpenHandles",
      "env": {
        "NODE_ENV": "test"
      }
    },
    "test:watch": {
      "command": "npm run test -- --watch"
    },
    "lint": {
      "command": "npx eslint --ext .js,.jsx ."
    }
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.6",
    "babel-jest": "^23.4.2",
    "babel-loader": "^7.1.5",
    "babel-plugin-dynamic-import-node": "^2.0.0",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-react-transform": "^3.0.0",
    "babel-plugin-styled-components": "^1.5.1",
    "babel-plugin-syntax-class-properties": "^6.13.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-syntax-object-rest-spread": "^6.13.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.14",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.26.0",
    "better-npm-run": "^0.1.1",
    "compression-webpack-plugin": "^1.1.11",
    "css-loader": "^1.0.0",
    "debug": "^3.1.0",
    "dotenv": "^6.0.0",
    "dotenv-safe": "^6.0.0",
    "dotenv-webpack": "^1.5.7",
    "enzyme": "^3.4.1",
    "enzyme-adapter-react-16": "^1.2.0",
    "enzyme-to-json": "^3.3.4",
    "escape-string-regexp": "^1.0.5",
    "eslint": "^5.3.0",
    "eslint-loader": "^2.1.0",
    "eslint-plugin-babel": "^5.1.0",
    "eslint-plugin-react": "^7.10.0",
    "expose-loader": "^0.7.5",
    "extract-css-chunks-webpack-plugin": "^3.0.11",
    "faker": "^4.1.0",
    "file-loader": "^1.1.11",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^23.5.0",
    "json-loader": "^0.5.7",
    "node-sass": "^4.9.3",
    "prettier-eslint": "^8.8.2",
    "react-hot-loader": "^4.3.4",
    "resolve-url-loader": "^2.3.0",
    "sass-loader": "^7.1.0",
    "sass-resources-loader": "^1.3.3",
    "sinon": "^6.1.5",
    "style-loader": "^0.22.1",
    "uglifyjs-webpack-plugin": "^1.2.7",
    "url-loader": "^1.0.1",
    "webpack": "^4.16.5",
    "webpack-bundle-analyzer": "^2.13.1",
    "webpack-hot-client": "^4.1.1",
    "webpack-manifest-plugin": "^2.0.3",
    "webpack-merge": "^4.1.4",
    "webpack-node-externals": "^1.7.2",
    "webpack-sources": "^1.1.0",
    "yn": "^2.0.0"
  },
  "dependencies": {
    "apollo-cache-inmemory": "^1.2.7",
    "apollo-client": "^2.3.8",
    "apollo-client-ssr-express": "0.1.0-alpha.8",
    "apollo-link": "^1.2.2",
    "apollo-link-context": "^1.0.8",
    "apollo-link-http": "^1.5.4",
    "cookie-parser": "^1.4.3",
    "dayjs": "^1.7.5",
    "formik": "^1.0.2",
    "graphql": "^0.13.2",
    "graphql-tag": "^2.9.2",
    "helmet": "^3.13.0",
    "i18n": "^0.8.3",
    "lodash": "^4.17.10",
    "module-alias": "^2.1.0",
    "query-string": "^6.1.0",
    "react": "^16.4.2",
    "react-apollo": "^2.1.11",
    "react-dom": "^16.4.2",
    "react-helmet": "^5.2.0",
    "react-loadable": "^5.5.0",
    "react-router-dom": "^4.3.1",
    "react-select": "^2.0.0",
    "rebass": "^2.1.0",
    "styled-components": "^3.4.2",
    "styled-system": "^3.0.2",
    "webpack-cli": "^3.1.0",
    "yup": "^0.26.2"
  }
}
