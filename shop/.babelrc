{
  "presets": [
    [
      "env",
      {
        "targets": {
          "node": "current",
          "uglify": false
        }
      }
    ],
    "react"
  ],
  "plugins": [
    "transform-es2015-modules-commonjs",
    "transform-class-properties",
    "transform-export-extensions",
    "transform-object-rest-spread",
    "dynamic-import-node",
    ["babel-plugin-styled-components", {
      "ssr": true,
      "displayName": true
    }]
  ],
  "env": {
    "test": {
      "presets": [
        "react"
      ]
    }
  }
}
